client
dev tun
proto tcp-client
remote vpn.foundryspatial.com
port 1194
nobind
persist-key
persist-tun
tls-client
remote-cert-tls server
verb 11
cipher AES-256-CBC
auth SHA1
# auth-user-pass
auth-nocache

script-security 2
up /etc/openvpn/update-systemd-resolved
up-restart
down /etc/openvpn/update-systemd-resolved
down-pre
dhcp-option DOMAIN-ROUTE .
